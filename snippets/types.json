{
	"aurora:element-source" : {
		"prefix" : "aurora:element-source",
		"body" : [
			"<element name=\"${1:name}\" type=\"Source\" source=\"Core\" id=\"ID_${3:author_source}_SOURCE_${1:name}\">",
			"\t<description>",
			"\t\t<![CDATA[<p>$0</p>]]>",
			"\t</description>",
			"\t<setters>",
			"\t\t<set name=\"abbreviation\">${5}</set>",
			"\t\t<set name=\"url\">${6}</set>",
			"\t\t<set name=\"image\">${7}</set>",
			"\t\t<set name=\"author\" abbreviation=\"${8:WOTC}\" url=\"${9:http://dnd.wizards.com}\">${10:Wizards of the Coast}</set>",
			"\t\t<set name=\"official\">${11|false,true|}</set>",
			"\t\t<set name=\"supplement\">${12|false,true|}</set>",
			"\t\t<set name=\"playtest\">${13|false,true|}</set>",
			"\t\t<set name=\"third-party\">${14|false,true|}</set>",
			"\t\t<set name=\"homebrew\">${15|false,true|}</set>",
			"\t\t<set name=\"league\">${16|false,true|}</set>",
			"\t\t<set name=\"release\">${17:20190521}</set>",
			"\t</setters>",
			"</element>"
		]
	},
	"aurora:element-spell" : {
		"prefix" : "aurora:element-spell",
		"body" : [
			"<element name=\"${1:name}\" type=\"Spell\" source=\"${2:source}\" id=\"ID_${3:author_source}_SPELL_${1:name}\">",
			"\t<supports>${supports}</supports>",
			"\t<description>",
			"\t\t<p>$0</p>",
			"\t</description>",
			"\t<setters>",
			"\t\t<set name=\"keywords\"></set>",
			"\t\t<set name=\"level\">${4:level}</set>",
			"\t\t<set name=\"school\">${5|Evocation,Conjuration,Enchantment,Transmutation,Illusion,Necromancy,Abjuration,Divination|}</set>",
			"\t\t<set name=\"time\">${6:time}</set>",
			"\t\t<set name=\"duration\">${7:duration}</set>",
			"\t\t<set name=\"range\">${8:range}</set>",
			"\t\t<set name=\"hasVerbalComponent\">${9|true,false|}</set>",
			"\t\t<set name=\"hasSomaticComponent\">${10|true,false|}</set>",
			"\t\t<set name=\"hasMaterialComponent\">${11|true,false|}</set>",
			"\t\t<set name=\"materialComponent\">${12:component}</set>",
			"\t\t<set name=\"isConcentration\">${13|false,true|}</set>",
			"\t\t<set name=\"isRitual\">${14|false,true|}</set>",
			"\t</setters>",
			"</element>"
		]
	},
	"aurora:element-language" : {
		"prefix" : "aurora:element-language",
		"body" : [
			"<element name=\"${1:name}\" type=\"Language\" source=\"${2:source}\" id=\"ID_${3:author_source}_LANGUAGE_${1:name}\">",
			"\t<description>",
			"\t\t<p>Typically spoken by $0.</p>",
			"\t</description>",
			"\t<setters>",
			"\t\t<set name=\"exotic\">${5|false,true|}</set>",
			"\t\t<set name=\"speakers\">${6}</set>",
			"\t\t<set name=\"script\">${7}</set>",
			"\t</setters>",
			"</element>"
		]
	},
	"aurora:element-background" : {
		"prefix" : "aurora:element-background",
		"body" : [
			"<element name=\"${1:name}\" type=\"Background\" source=\"${2:source}\" id=\"ID_${3:author_source}_BACKGROUND_${1:name}\">",
			"\t<description>",
			"\t\t<p>$0</p>",
			"\t\t<p class=\"indent\"></p>",
			"\t\t<ul class=\"unstyled\">",
			"\t\t\t<li><strong>Skill Proficiencies:</strong> </li>",
